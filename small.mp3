proxy-groups:
- name: Final
  type: select
  disable-udp: false
  use:
  - mesl
  - planb
  - planc
  filter: (?<=0.{2})X
- name: EMBY
  type: select
  disable-udp: false
  proxies:
  - Final
  - DIRECT
  use:
  - mesl
  - planb
  - planc
  filter: ^$
rules:
- DST-PORT,7895,REJECT
- DST-PORT,7892,REJECT
- IP-CIDR,198.18.0.1/16,REJECT,no-resolve
- RULE-SET,国内流媒体合集,DIRECT
- RULE-SET,国内域名白名单,DIRECT
- RULE-SET,emby,EMBY
- RULE-SET,国内IP白名单,DIRECT
- MATCH,Final
